<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    >
    <FrameLayout
        android:layout_width="@dimen/view_menu_item_size"
        android:layout_height="@dimen/view_menu_item_size"
        android:layout_margin="@dimen/view_normal_margin_narrow"
        android:background="@drawable/menu_background_1"
        >
        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="@string/app_name"
            android:singleLine="true"
            android:textColor="@color/mainColorHelp_default"
            android:layout_gravity="center"
            android:textSize="@dimen/textView_normal_textSize_narrow" />

    </FrameLayout>

</FrameLayout>